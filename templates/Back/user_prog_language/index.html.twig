{% extends 'back_base.html.twig' %}

{% block title %}UserProgLanguage index{% endblock %}

{% block content %}
    <h1>UserProgLanguage index</h1>

    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th>Id</th>
            <th>User</th>
            <th>Prog Language</th>
            <th>Level</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for user_prog_language in user_prog_languages %}
            <tr>
                <td>{{ user_prog_language.id }}</td>
                <td>{{ user_prog_language.user.email }}</td>
                <td>{{ user_prog_language.progLanguage.name }}</td>
                <td>{{ user_prog_language.level }}</td>
                <td>
                    <a class="btn btn-primary"
                       href="{{ path('back_user_prog_language_show', {'id': user_prog_language.id}) }}">show</a>
                    <a class="btn btn-primary"
                       href="{{ path('back_user_prog_language_edit', {'id': user_prog_language.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
