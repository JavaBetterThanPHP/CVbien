{% extends 'back_base.html.twig' %}

{% block title %}Link{% endblock %}

{% block content %}
    <h1>Link</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ link.id }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ link.website }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ link.url }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ link.name }}</td>
            </tr>
            <tr>
                <th>ImageUrl</th>
                <td>{{ link.imageUrl }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ link.user.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_link_index') }}">back to list</a>
    <br>
    <a href="{{ path('back_link_edit', {'id': link.id}) }}">edit</a>
{% endblock %}
