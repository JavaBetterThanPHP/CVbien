{% extends 'back_base.html.twig' %}

{% block title %}ProgTechnology{% endblock %}

{% block content %}
    <h1>ProgTechnology</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ prog_technology.id }}</td>
        </tr>
        <tr>
            <th>Label</th>
            <td>{{ prog_technology.label }}</td>
        </tr>
        <tr>
            <th>Related language</th>
            <td>{% if prog_technology.progLanguage.name  is defined %}{{ prog_technology.progLanguage.name }}{% endif %}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('back_prog_technology_index') }}">back to list</a>
    <br>
    <a href="{{ path('back_prog_technology_edit', {'id': prog_technology.id}) }}">edit</a>

    {{ include('Back/prog_technology/_delete_form.html.twig') }}
{% endblock %}
