{% extends 'back_base.html.twig' %}

{% block title %}UserLanguage{% endblock %}

{% block content %}
    <h1>UserLanguage</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ user_language.id }}</td>
        </tr>
        <tr>
            <th>Language</th>
            <td>{{ user_language.language.name }}</td>
        </tr>
        <tr>
            <th>User</th>
            <td>{{ user_language.user.email }}</td>
        </tr>
        <tr>
            <th>Level</th>
            <td>{{ user_language.level }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('back_user_language_index') }}">back to list</a>

    <a href="{{ path('back_user_language_edit', {'id': user_language.id}) }}">edit</a>

{% endblock %}
