{% extends 'back_base.html.twig' %}

{% block title %}ProgLanguage{% endblock %}

{% block content %}
    <h1>ProgLanguage</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ prog_language.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ prog_language.name }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ prog_language.type }}</td>
        </tr>
        <tr>
            <th>Related technologies</th>
            <td>
                <ul>
                    {% for technology in prog_language.progTechnologies  %}
                        <li>{{ technology.label }}</li>
                    {% endfor %}
                </ul>
            </td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('back_prog_language_index') }}">back to list</a>
    <br>
    <a href="{{ path('back_prog_language_edit', {'id': prog_language.id}) }}">edit</a>

    {{ include('Back/prog_language/_delete_form.html.twig') }}
{% endblock %}
